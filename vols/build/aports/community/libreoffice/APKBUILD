# openjdk isn't available on 32-bit
case $CARCH in
x86|arm*|aarch64)
	_have_java=false
	;;
*)
	_have_java=true
	makedepends="$makedepends openjdk11-jdk openjdk11-jre-headless"
	;;
esac
