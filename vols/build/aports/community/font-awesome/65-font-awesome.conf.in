<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
<fontconfig>
  <alias>
    <family>fantasy</family>
    <prefer>
      <family>Font Awesome @@VERSION@@ Free</family>
    </prefer>
  </alias>

  <alias>
    <!-- Non-standard generic for icon fonts -->
    <family>icon</family>
    <prefer>
      <family>Font Awesome @@VERSION@@ Free</family>
    </prefer>
  </alias>
</fontconfig>
